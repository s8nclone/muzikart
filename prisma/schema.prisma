generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  user_id       BigInt @id @default(autoincrement())
  username      String @unique @db.VarChar(255)
  email         String @unique @db.VarChar(255)
  password_hash String @db.Char(60)
}
